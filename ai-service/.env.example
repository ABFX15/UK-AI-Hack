# AI Service Environment Variables

# REQUIRED: OpenAI API for AI-powered analysis
OPENAI_API_KEY=your_openai_key_here

# OPTIONAL: CoinGecko API for real-time market data (has free tier)
COINGECKO_API_KEY=your_coingecko_api_key_here

# OPTIONAL: TheGraph API for DeFi protocol data
THEGRAPH_API_KEY=your_thegraph_api_key_here

# OPTIONAL: Chainalysis API for AML/KYC compliance
CHAINALYSIS_API_KEY=your_chainalysis_api_key_here

# Legacy
GITHUB_TOKEN=your_github_token_here
REDIS_URL=redis://localhost:6379
MAIN_API_URL=http://localhost:3000

# Service Configuration
AI_SERVICE_PORT=8001
AI_SERVICE_HOST=0.0.0.0

# Development
ENVIRONMENT=development
LOG_LEVEL=INFO
